        <h2>Started jquery-ui</h2>
        <input type="text" id="myinput" placeholder="Click to select a date">
    </div>
</body>
<script src="jquery.js" type="text/javascript"></script>
<script src="jqueryui/jquery-ui.js" type="text/javascript"></script>

<script>
    $("#myinput").datepicker({
        numberOfMonths:1,
        changeYear:true,
        changeMonth:true,
        showWeek:true,
        showOtherMonths:true,
        minDate : new Date(2017,0,1),
        maxDate : new Date(2023,0,1),
    });
    
</script>
